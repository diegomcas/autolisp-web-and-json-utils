  (princ "list of json3 -> ")(princ lst_json3) (princ "\n")
  (princ "list of json3 without <ARRAY> Tags -> ")(princ (dmc:json:json_to_list json3 nil)) (princ "\n")
  (princ "--------------------------------------------------------------------------------------\n")
  (princ "json4 -> ") (princ json4) (princ "\n")
  (setq lst_json4 (dmc:json:json_to_list json4 T))
  (princ "list of json4 -> ")(princ lst_json4) (princ "\n")
  (princ "list of json4 without <ARRAY> Tags -> ")(princ (dmc:json:json_to_list json4 nil)) (princ "\n")
  (princ "--------------------------------------------------------------------------------------\n")
  (princ "json5 -> ") (princ json5) (princ "\n")
