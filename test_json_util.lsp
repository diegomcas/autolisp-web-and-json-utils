  (princ "list of json5 -> ")(princ lst_json5) (princ "\n")
  (princ "list of json5 without <ARRAY> Tags -> ")(princ (dmc:json:json_to_list json5 nil)) (princ "\n")
  (princ "--------------------------------------------------------------------------------------\n")
  (princ "json6 -> ") (princ json5) (princ "\n")
  (setq lst_json6 (dmc:json:json_to_list json6 T))
  (princ "list of json6 -> ")(princ lst_json6) (princ "\n")
  (princ "list of json6 without <ARRAY> Tags -> ")(princ (dmc:json:json_to_list json6 nil)) (princ "\n")
  (princ "--------------------------------------------------------------------------------------\n")

  (princ "------------------------------The lists to json---------------------------------------\n")
  (princ "json of lst_json1 -> ") (princ (dmc:json:list_to_json lst_json1)) (princ "\n")
  (princ "--------------------------------------------------------------------------------------\n")
  (princ "json of lst_json2 -> ") (princ (dmc:json:list_to_json lst_json2)) (princ "\n")
  (princ "--------------------------------------------------------------------------------------\n")
  (princ "json of lst_json3 -> ") (princ (dmc:json:list_to_json lst_json3)) (princ "\n")
  (princ "--------------------------------------------------------------------------------------\n")
  (princ "json of lst_json4 -> ") (princ (dmc:json:list_to_json lst_json4)) (princ "\n")
  (princ "--------------------------------------------------------------------------------------\n")
  (princ "json of lst_json5 -> ") (princ (dmc:json:list_to_json lst_json5)) (princ "\n")
  (princ "--------------------------------------------------------------------------------------\n")
  (princ "json of lst_json6 -> ") (princ (dmc:json:list_to_json lst_json6)) (princ "\n")
  (princ "--------------------------------------------------------------------------------------\n")
  (princ)
)
